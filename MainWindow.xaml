        <!--#region Page de presentation -->
        <Image x:Name="FDE" Source="/Images_Fond/fond necron 1600 900.png " HorizontalAlignment="Center" VerticalAlignment="Center" OpacityMask="#FF67E80E" Grid.ColumnSpan="2" Margin="0,0,0,0" Visibility="collapsed"/>
        <StackPanel x:Name="StackPanel_CPV9_Droit" Orientation="Vertical" Visibility="Collapsed">
            <Label x:Name="CDV9_droit_Neo" Content="Néo" Width="Auto" Height="Auto" Margin="0,14,0,0" HorizontalAlignment="Right" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Background="{x:Null}" FontSize="100" FontFamily="Arial Black">
                <Label.Foreground>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="Black" Offset="0.087"/>
                        <GradientStop Color="#FF73F10A" Offset="1"/>
                    </LinearGradientBrush>
                </Label.Foreground>
                <Label.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="10.372"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Label.RenderTransform>
            </Label>
            <Label x:Name="CDV9_droit_ScoreBoard" Content="ScoreBoard" Width="Auto" Height="Auto" Margin="0,-40,0,0" HorizontalAlignment="Right" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5" Background="{x:Null}" FontSize="30" FontFamily="Arial Black">
                <Label.Foreground>
                    <RadialGradientBrush>
                        <GradientStop Color="#FF1244EE" Offset="1"/>
                        <GradientStop Color="#FFFF1700"/>
                    </RadialGradientBrush>
                </Label.Foreground>
                <Label.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="10.372"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Label.RenderTransform>
            </Label>
        </StackPanel>
        <StackPanel x:Name="StackPanel_CPV9_Gauche" Orientation="Vertical" Visibility="Collapsed">
